{
  "manifest_version": 2,

  "name": "Chart Reloader",
  "description": "Reload all stock chart pages.",
  "version": "1.0",
  "background": {
    "scripts": ["lib/lodash-3.10.1.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "reload.png",
    "default_title": "Reload all stock charts"
  },
  "permissions": [
    "tabs"
  ]
}
